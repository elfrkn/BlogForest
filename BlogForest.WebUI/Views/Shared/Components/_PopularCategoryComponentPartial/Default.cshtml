@model List<BlogForest.DtoLayer.CategoryDtos.ResultCategoryWithCountDto>
<div class="widget">
    <h2 class="widget-title">Popular Categories</h2>
    <div class="link-widget">
        <ul>
            @foreach(var item in Model)
            {
                <li><a href="#">@item.CategoryName<span>@item.CategoryCount</span></a></li>
            }
           
          
        </ul>
    </div><!-- end link-widget -->
</div>